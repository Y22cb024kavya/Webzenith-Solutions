{
  "name": "rbac-system",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "set NODE_OPTIONS=--dns-result-order=ipv4first && turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}